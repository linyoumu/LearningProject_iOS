# Uncomment the next line to define a global platform for your project
project 'LearningProject_iOS.xcodeproj'
platform :ios, '11.0'

target 'LearningProject_iOS' do
  use_frameworks!

  #网络请求框架
  pod 'AFNetworking'
  pod 'MJRefresh'
  pod 'GTMBase64'
  pod 'Masonry'
  pod 'SVProgressHUD' # Api相对于MBProgressHUD来说简便一些
  pod 'SDWebImage'
  pod 'YYKit'
  pod 'NullSafe'
#  pod 'GBDeviceInfo'

  target 'LearningProject_iOSTests' do
    inherit! :search_paths
  end

  target 'LearningProject_iOSUITests' do
  end

end
